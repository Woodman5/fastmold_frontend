{
    "extends": "@quasar/app/tsconfig-preset",
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "baseUrl": ".",
        // это обеспечивает более строгий вывод для свойств данных в `this`
        "strict": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "noImplicitAny": false
    }
}
